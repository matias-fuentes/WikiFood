{
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/helpers.py"
        }
    ]
}
